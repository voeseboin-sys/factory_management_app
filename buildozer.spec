#android.res_xml = path/to/file.xml

# (str) FileProvider authority (para compartir archivos en Android 7+)
android.fileprovider_authority = com.factorymanager.app.fileprovider

# (str) Android app entry point
#android.entrypoint = org.kivy.android.PythonActivity

# (list) Android archs to build for, choices: armeabi-v7a, arm64-v8a, x86, x86_64
# In past, was `android.arch` as we weren't supporting builds for multiple archs at the same time.
