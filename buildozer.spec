#android.meta_data =

# (list) Android library project to add (will be added in the project.properties automatically)
#android.library_references =

# (list) Android shared libraries which will be added to AndroidManifest.xml using <uses-library> tag
#android.uses_library =

# (str) Android logcat filters to use
#android.logcat_filters = *:S python:D

# (bool) Android logcat only display log for activity's pid instead of full log
#android.logcat_pid_only = True

# (str) Android additional build commands
#android.additional_build_commands =

# (str) Android gradle plugin to use. Please specify the full path and version.
#android.gradle_plugin =

# (list) Android Java classes to extend activity classes
#android.add_activities =

